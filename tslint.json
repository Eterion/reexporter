{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "curly": [true, "ignore-same-line"],
    "no-console": false,
    "no-shadowed-variable": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
