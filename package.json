{
  "name": "reexporter",
  "version": "1.0.3",
  "description": "Manage index files for ES6 modules and other exports.",
  "keyword": [
    "auto",
    "es6",
    "esm",
    "export",
    "generate",
    "import",
    "index",
    "module"
  ],
  "bin": "bin.js",
  "main": "index.js",
  "typings": "index",
  "repository": "https://github.com/Eterion/reexporter.git",
  "author": "Eterion <Eterion@users.noreply.github.com>",
  "license": "MIT",
  "files": [
    "bin.js",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "build": "webpack -r tsconfig-paths/register --mode development",
    "clean": "del-cli \"bin.js\" \"index.js\"",
    "prepack": "yarn release",
    "prettier": "prettier --write \"**/*.{ts,md}\"",
    "release": "webpack -r tsconfig-paths/register --mode production",
    "test": "mocha \"test/**/*.{test,spec}.ts\" -r ts-node/register -r tsconfig-paths/register",
    "tslint": "tslint -p tsconfig.json -t stylish --fix",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "glob": "^7.1.2",
    "path": "^0.12.7",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/glob": "^5.0.35",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.7",
    "@types/webpack": "^4.4.9",
    "@types/webpack-node-externals": "^1.6.3",
    "@types/yargs": "^11.1.1",
    "chai": "^4.1.2",
    "del-cli": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.14.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.5.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
